{
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "python.terminal.activateEnvironment": true,
    "python.defaultInterpreterPath": "${env:VIRTUAL_ENV}",
    "cSpell.words": [
        "endcall",
        "endset",
        "flowbite",
        "htmx",
        "irender",
        "jinjax",
        "navitems",
        "systest",
        "tailwindcss",
        "unstage",
        "unstaged",
        "venv",
        "watchcss",
        "webdev"
    ],
    "markdown.extension.toc.updateOnSave": false,
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#afa5d2",
        "activityBar.background": "#afa5d2",
        "activityBar.foreground": "#15202b",
        "activityBar.inactiveForeground": "#15202b99",
        "activityBarBadge.background": "#955b4a",
        "activityBarBadge.foreground": "#e7e7e7",
        "commandCenter.border": "#15202b99",
        "sash.hoverBorder": "#afa5d2",
        "statusBar.background": "#9183c1",
        "statusBar.foreground": "#15202b",
        "statusBarItem.hoverBackground": "#7361b0",
        "statusBarItem.remoteBackground": "#9183c1",
        "statusBarItem.remoteForeground": "#15202b",
        "titleBar.activeBackground": "#9183c1",
        "titleBar.activeForeground": "#15202b",
        "titleBar.inactiveBackground": "#9183c199",
        "titleBar.inactiveForeground": "#15202b99"
    },
    "peacock.color": "#9183c1",
    "files.exclude": {
        "**/__pycache__": true,
        "**/.pytest_cache": true,
        "**/*.egg-info": true
    }
}